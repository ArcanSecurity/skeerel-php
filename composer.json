{
  "name": "arcansecurity/skeerel-php",
  "description": "Skeerel PHP Library",
  "keywords": [
    "skeerel",
    "arcansecurity",
    "php",
    "passwordless authentication",
    "api",
    "payment"
  ],
  "homepage": "https://skeerel.com/",
  "license": "MIT",
  "authors": [
    {
      "name": "ArcanSecurity and contributors",
      "homepage": "https://github.com/ArcanSecurity/skeerel-php/graphs/contributors"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "ext-json": "*"
  },
  "suggest": {
    "ext-openssl": "Install the OpenSSL extension in order to generate better random bits.",
    "ext-mcrypt": "Install the Mcrypt extension in backup of openssl."
  },
  "autoload": {
    "psr-4": { "Skeerel\\" : "Skeerel/" }
  }
}